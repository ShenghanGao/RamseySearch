#include <stdlib.h>
#include <stdio.h>
#include <errno.h>
//#include <stdbool.h>
#include <string.h>
#include "list.h"
#include "fifo.h"	/* for taboo list */
#include "time.h"
#include "clique_count.h"
#define MAXSIZE (541)
#define TABOOSIZE (250)
#define BIGCOUNT (1000)
#define THRESHOLD (10)
/*
 * example of very simple search for R(7, 7) counter 
 * examples starts with a small randomized graph and 
 * works its way up to successively larger graphs one 
 * at a time
 * uses a taboo list of size #TABOOSIZE# to hold and 
 * encoding of and edge (i, j) + clique_count
 */
typedef enum {false, true} bool;

int main(int argc, char *argv[])
{
	int *g, *g1;
	int gsize, gsize1, i, j; 
	
	ReadGraph("../../ce_big", &g, &gsize);
	ReadGraph("../../ce_small", &g1, &gsize1);
		
	for(i = 0; i < gsize1 / 2; i ++) {
		for(j = i + 1; j < gsize1 / 2; j ++) {
			g[i * gsize + j] = g1[i * gsize1 + j];
		}
	}
	PrintGraph(g, gsize);
	return(0);
}

















